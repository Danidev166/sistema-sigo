{
  "name": "sigo-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:analyze": "vite build --mode analyze",
    "build:prod": "vite build --mode production",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "test": "vitest",
    "test:run": "vitest run --reporter=basic",
    "test:run:single": "vitest run --reporter=basic --run src/test/components/UserTable.test.jsx",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:integration": "vitest run --config vite.config.js --reporter=verbose",
    "test:ci": "npm run lint && npm run test:run && npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "optimize": "node scripts/optimize-performance.js",
    "analyze": "npm run build:analyze && npx vite-bundle-analyzer dist",
    "lighthouse": "npx lighthouse http://localhost:5173 --output=html --output-path=./lighthouse-report.html",
    "performance": "npm run optimize && npm run build && npm run lighthouse",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "ci": "npm run lint && npm run build && npm run audit",
    "ci:full": "npm run lint && npm run test:run && npm run build && npm run audit"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@tabler/icons-react": "^3.33.0",
    "axios": "^1.6.2",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.12.1",
    "lucide-react": "^0.511.0",
    "papaparse": "^5.5.3",
    "pdfkit": "^0.17.1",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-qr-code": "^2.0.18",
    "react-router-dom": "^6.20.0",
    "react-select": "^5.10.1",
    "recharts": "^2.15.3",
    "sonner": "^2.0.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.4",
    "@babel/preset-react": "^7.27.1",
    "@chromatic-com/storybook": "^4.0.1",
    "@eslint/js": "^9.35.0",
    "@storybook/addon-a11y": "^9.0.14",
    "@storybook/addon-docs": "^9.0.14",
    "@storybook/addon-onboarding": "^9.0.14",
    "@storybook/react-vite": "^9.0.14",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.1",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/ui": "^3.2.4",
    "audit-ci": "^7.1.0",
    "autoprefixer": "^10.4.21",
    "cypress": "^13.6.0",
    "eslint": "^9.25.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^9.0.14",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "jsdom": "^22.1.0",
    "lighthouse": "^11.6.0",
    "msw": "^2.11.1",
    "postcss": "^8.5.3",
    "prettier": "^3.6.2",
    "prop-types": "^15.8.1",
    "storybook": "^9.0.14",
    "tailwindcss": "^3.4.17",
    "terser": "^5.42.0",
    "vite": "^6.3.5",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^3.2.4"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
